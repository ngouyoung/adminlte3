{
    "name": "wemade/admin-lte3",
    "description": "this package is relate to laravel admin with user assessment",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wemade\\AdminLte3\\": "src/"
        },
        "files": [
            "src/app/Helpers/helpers.php",
            "src/app/Helpers/routes.php",
            "src/app/Helpers/buttons.php"
        ]
    },
    "authors": [
        {
            "name": "skip"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "realrashid/sweet-alert": "^5.0",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables-oracle": "^9.19",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wemade\\AdminLte3\\AdminServiceProvider"
            ]
        }
    }
}
